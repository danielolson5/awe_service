#!/bin/bash
export KB_TOP=[% kb_top %]
export KB_RUNTIME=[% kb_runtime %]
export PATH=$KB_TOP/bin:$KB_RUNTIME/bin:$PATH
export PERL5LIB=$KB_TOP/lib
export KB_SERVICE_DIR=$KB_TOP/services/[% kb_service_dir %]
export KB_DEPLOYMENT_CONFIG=$KB_TOP/deployment.cfg
export R_LIBS=$KB_TOP/lib

AWE_DIR=[% kb_top %]/services/awe_service
PID_FILE=$AWE_DIR/client.pid
AWEC_CONFIG=[% kb_top %]/services/awe_service/conf/awec.cfg

echo "[% kb_runtime %]/sbin/daemonize -v -o $AWE_DIR/awec-start.log -e $AWE_DIR/awec-start.log -p $PID_FILE [% kb_top %]/bin/awe-client -conf $AWEC_CONFIG"
[% kb_runtime %]/sbin/daemonize -v -o $AWE_DIR/awec-start.log -e $AWE_DIR/awec-start.log -p $PID_FILE [% kb_top %]/bin/awe-client -conf $AWEC_CONFIG
